{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "import/no-unresolved": [0],
    "no-mixed-requires": [0],
    "no-mixed-operators": [0],
    "class-methods-use-this": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [0],
    "no-unused-expressions": [0],
    "no-param-reassign": [0],
    "no-case-declarations": [0],
    "no-alert": [0],
    "arrow-body-style": [0],
    "semi": [0],
    "consistent-return": [0],
    "max-len": [0],
    "prefer-template": [0],
    "import/no-extraneous-dependencies": [0],
    "import/extensions": [0]
  }
}